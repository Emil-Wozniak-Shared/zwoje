<idea-plugin>
    <id>pl.ejdev.ZwojeIJPlugin</id>
    <name>Zwoje IJ</name>
    <vendor url="https://www.ejdev.pl">EJ Dev</vendor>
    <description><![CDATA[
        This plugin provides possibility for a developer to see how PDF rendered
        from HTML template will look like without necessity to start application.
    ]]></description>
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.json</depends>
    <extensions defaultExtensionNs="com.intellij">
        <toolWindow
                id="Zwoje IJ"
                factoryClass="pl.ejdev.zwoje.window.ZwojeWindowFactory"
        />
<!--                icon="/icons/favicon.svg"-->
        <editorNotificationProvider implementation="pl.ejdev.zwoje.notifications.ZwojeEditorNotificationProvider"/>
    </extensions>
</idea-plugin>